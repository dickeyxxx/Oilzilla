- form_for @car do |f|
  %fieldset
    =f.error_messages
    %p
      =f.label :make_string
      =f.text_field :make_string
    %p
      =f.label :model_string
      =f.text_field :model_string
    %p
      =f.label :auto_year_int
      =f.text_field :auto_year_int
    %p
      =f.label :trim_string
      =f.text_field :trim_string
    %input{ :type => "submit", :value => "Add"}
